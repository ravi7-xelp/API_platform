case 'PMC_data':
    return array_map(function (user) {
        return [
            'ID' => user['id'],
            'PPRID' => user['PPRID'],
            'Title' => user['Title'],
            'Authors' => user['Authors'],
            'Citation' => user['Citation'],
            'First_Author' => user['First_Author'],
            'Preprint' => user['Preprint'],
            'Publication_Year' => user['Publication_Year'],
            'Create_Date' => user['Create_Date'],
            'DOI' => user['DOI'],
            'Full_Text' => user['Full_Text'],
            'URL' => user['URL'],
            'Is_Preprint' => (user['Preprint'] !== '') ? true : false,
        ];
    }, users);



case 'PMCcompany_presentation_publication_data':
    return array_map(function ($user) {
        return [
            'ID' => $user['ID'],
            'Company' => $user['Company'],
            'Website_Link' => $user['Website Link'],
            'Pdf_Link' => $user['Pdf Link'],
            'File_Name' => $user['File Name'],
            'Title' => $user['Title'],
            'Status' => $user['Status'],
            'First_Page' => $user['First Page'],
            'Full_Text' => $user['Full Text'],
            'URL' => $user['URL'],
            'Is_Public' => ($user['Status'] === 'Active') ? true : false,
        ];
    }, $users);

        
case 'company_presentation_publication':
    return array_map(function ($user) {
    return [
        'ID' => $user['ID'],
        'Company' => $user['Company'],
        'Website Link' => $user['Website Link'],
        'Pdf Link' => $user['Pdf Link'],
        'File Name' => $user['File Name'],
        'Title' => $user['Title'] ?? null,
        'Status' => $user['Status'],
        'First Page' => $user['First Page'],
        'Full Text' => $user['Full Text'],
        'Presentation Reference' => $user['Presentation Reference'],
        'Key Information' => $user['Key Information'] ?? null,
    ];
}, $users);


