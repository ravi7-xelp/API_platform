case 'ClincalDatatUS':
    return array_map(function ($user) {
        return [
            'ID' => $user['id'],
            'Catailyst Drug Candidate ID' => true,
            'Drug Name (original)' => $user['Title'],
            'AssetName' => $user['Acronym'],
            'Company' => $user['SponsorCollaborators'],
            'Alt Names' => $user['OtherIDs'],
            'Latest Update' => $user['LastUpdatePosted'],
            'Originator' => null,
            'Developer' => null,
            'Class' => null,
            'Therapeutic Modality' => $user['StudyType'],
            'Mechanism of Action' => null,
            'New Molecular Entity' => false,
            'Indication' => $user['Conditions'],
            'Development Stage' => $user['Phases'],
            'Recent News' => null,
            'Category' => null,
            'Dev Stage Rank' => null,
            'Therapeutic Area' => null,
            'Orphan Drug' => false,
            'Research Program' => null
        ];
    }, $users);


